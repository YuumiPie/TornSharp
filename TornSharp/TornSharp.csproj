<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>TornSharp</RootNamespace>
        <Title>TornSharp</Title>
        <Authors>YuumiPie</Authors>
        <Description>A C# wrapper for the Torn City api</Description>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageProjectUrl>https://github.com/YuumiPie/TornSharp</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/YuumiPie/TornSharp/blob/develop/LICENSE.md</PackageLicenseUrl>
        <RepositoryType>git</RepositoryType>
        <Copyright>Copyright (c) YuumiPie 2024-2024</Copyright>
        <PackageTags>TornCity Wrapper</PackageTags>
        <Version>1.0.5</Version>
        <PackageReleaseNotes>Fixed missing/mistyped properties in some models</PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="EnumExtensions.System.Text.Json" Version="1.2.0"/>
    </ItemGroup>

</Project>
